<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="CustomItemProperty">
    <attr name="title_item" format="string"/>
    <attr name="content_item" format="string"/>
  </declare-styleable>

  <declare-styleable name="CustomItemSummary">
    <attr name="title_item_summary" format="string"/>
    <attr name="content_item_summary" format="string"/>
    <attr name="content_item_bold" format="boolean"/>
    <attr name="text_color_item_summary" format="integer"/>
    <attr name="icon_next_summary" format="reference"/>
    <attr name="second_icon" format="reference"/>
    <attr name="icon_status" format="reference"/>
  </declare-styleable>

  <declare-styleable name="ProcessTracker">
    <attr name="overall_process_title" format="string"/>
    <attr name="hide_top_line" format="boolean"/>
    <attr name="hide_bottom_line" format="boolean"/>
    <attr name="number_of_process" format="integer"/>
    <attr name="first_inner_process_title" format="string"/>
    <attr name="second_inner_process_title" format="string"/>
  </declare-styleable>

  <declare-styleable name="CustomProfileForm">
    <attr name="form_title" format="string"/>
    <attr name="is_editable" format="boolean"/>
    <attr name="action_image" format="reference"/>
    <attr name="title_text_color" format="integer" />
    <attr name="content_text_color" format="integer" />
  </declare-styleable>
</resources>